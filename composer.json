{
    "name": "joshuastaff/laravel-idempotency",
    "description": "An easily customisable and reusable idempotency middleware for Laravel.",
    "require": {
        "php": ">=5.4.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelIdempotency\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelIdempotency\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
